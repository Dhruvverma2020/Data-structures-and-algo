#include <iostream>
#include<bits/stdc++.h>
using namespace std;
void print(string s,int n)
{
    int count=0;
    while(count<n)
    {
        if(count==n)
        {
            return;
        }
        cout<<s<<endl;
        count++;
    }
}
int main()
{
    string s;
    cin>>s;
    int n;
    cin>>n;
    print(s,n);

    return 0;
}
