#include <iostream>
#include<bits/stdc++.h>
using namespace std;
void addedge(vector<int>adj[],int u,int v)
{
    adj[u].push_back(v);
    adj[v].push_back(u);
}
void printgraph(vector<int>adj[],int v)
{
    for(int i=0;i<v;i++)
    {
        for(auto x:adj[i])
        {
            cout<<i<<"->"<<x<<" ";
        }
        cout<<"\n";
    }
}
int main()
{
    int v=4;
    vector<int>adj[v];
    addedge(adj,0,1);
    addedge(adj,0,2);
    addedge(adj,1,2);
    addedge(adj,2,3);
    printgraph(adj,v);

    return 0;
}
//output looks like this
0->1 0->2 
1->0 1->2 
2->0 2->1 2->3 
3->2 
